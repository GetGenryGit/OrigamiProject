<Router AppAssembly="@typeof(Program).Assembly" AdditionalAssemblies="new[] { typeof(WebComponents._Imports).Assembly }">
    <Found Context="routeData">
        <RouteView RouteData="@routeData" DefaultLayout="@typeof(WebComponents.Components.Layout.MainLayout)" />
        <FocusOnNavigate RouteData="@routeData" Selector="h1" />
    </Found>
</Router>
